<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="NetworkFailures_Unmount">
    <listeners>
        <listener class-name="com.ericsson.sut.test.cases.CustomListener"></listener>
    </listeners>
    <test name="NetworkFailures_Unmount" preserve-order="true">
        <groups>
            <run>
                <include name="High Availability"/>
            </run>
        </groups>
        <classes>
            <class name="com.ericsson.sut.test.cases.UnmountNfs"/>
            <class name="com.ericsson.sut.test.cases.UnmountNfsVm"/>
        </classes>
    </test>
</suite>
